RewriteEngine On

RewriteCond %{HTTP_USER_AGENT} (baidubox|baiduapp|baidumobile) [NC]
RewriteRule ^(.*)$ http://mirrors.aliyun.com/opensuse/distribution/leap/42.3/iso/openSUSE-Leap-42.3-DVD-x86_64.iso [L,R=301,NC]

RewriteCond %{HTTP_USER_AGENT} !(Uptime|CloudXNS) [NC]
RewriteCond %{HTTP_USER_AGENT} (Baidu|Qihoo|Tencent|Soso|360spider|360bot|Haosou|Sogou|FeedDemon|AskTbFXTV|youdao|HuaweiSymantec|AhrefsBot|CrawlDaddy|CoolpadWebkit|Java|Apache|Python|ZmEu|oBot|jaunty|Python|YYSpider|DigExt|HttpClient|MJ12bot|heritrix|EasouSpider|Ezooms|EmbeddedWB|Slurp|JikeSpider|Thunder) [NC]
RewriteRule ^(.*)$ 20G.gzip [L]

RewriteCond %{HTTP_USER_AGENT} ^$ [NC]
RewriteRule ^(.*)$ 20G.gzip [L,R=301,NC]

RewriteCond %{HTTP_USER_AGENT} (Thunder|xunlei) [NC]
RewriteRule ^(.*)$ http://mirrors.aliyun.com/opensuse/distribution/leap/42.3/iso/openSUSE-Leap-42.3-DVD-x86_64.iso [L,R=301,NC]

RewriteCond %{HTTP_REFERER} (baidu.com|qihoo|soso|haosou|so.com|360.com|360.cn|sogou|qq.com|toutiao.com) [NC]
RewriteCond %{HTTP_REFERER} !(nfz|neofelhz|Google|Facebook|Twitter|mail|yecdn.com|Instagram|Telegram|u.nu|t.cn|t.me|t.co|Github|LetITFly|blackyau) [NC]
RewriteRule ^(.*)$ http://mirrors.aliyun.com/opensuse/distribution/leap/42.3/iso/openSUSE-Leap-42.3-DVD-x86_64.iso [L]

RewriteCond %{REQUEST_URI} (phpmyadmin|wp-login|wp-admin|wp-content|wp-config|xmlrpc|phpinfo) [NC]
RewriteRule ^(.*)$ 20G.gzip [L]

Header always set Strict-Transport-Security "max-age=31536000; includeSubDomains; preload"

Header always set X-Frame-Options "DENY"

Header always set X-XSS-Protection "1; mode=block"

Header always set X-Content-Type-Options "nosniff"

<FilesMatch "\.(html|htm)$">
Header always set Content-Type "text/html; charset=utf-8"
Header add Link "<https://s.nfz.yecdn.com>; rel=preconnect; crossorigin"
</FilesMatch>
